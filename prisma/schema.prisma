generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Transaction {
  id         Int      @id @default(autoincrement())
  date       DateTime
  item       String
  qty        Float
  unit       String?
  price      Int?
  total      Int?
  type       String?
  created_at DateTime @default(now())
}
